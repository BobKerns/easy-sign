{
  "name": "npm-template",
  "version": "0.1.0",
  "description": "Template project",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.mjs",
  "typings": "module-typings.d.ts",
  "scripts": {
    "config": "tsc --project config/tsconfig.json",
    "predevtools": "npm run config",
    "devtools": "tsc --project devtools/tsconfig.json",
    "pretest": "npm run build",
    "test": "jest",
    "prebuild": "npm run devtools",
    "build": "rollup -c",
    "clean": "rm -rf build lib .tsbuild-info  devtools/build devtools/lib devtools/.tsbuild-info config/build config/lib config/.tsbuild-info",
    "prepare": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BobKerns/row-padding.git"
  },
  "keywords": [
    "template",
    "node",
    "typescript"
  ],
  "author": "Bob Kerns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BobKerns/row-padding/issues"
  },
  "homepage": "https://github.com/BobKerns/row-padding#readme",
  "dependencies": {
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "wcsize": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "jest": "^24.8.0",
    "rollup": "^1.12.3",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-visualizer": "^1.1.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  }
}
